/mnt/log/rails/*log {
    daily
    rotate 10
    missingok
    notifempty
    sharedscripts
    postrotate
        su - webuser -c '/home/webuser/tapjoyserver/server/start_or_reload_unicorn.rb'
        su - webuser -c 'cd /home/webuser/tapjoyserver/ && server/restart_job_daemon.rb'
    endscript
}
