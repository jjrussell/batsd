!!! Mobile

%html{ :class => 'no-js no-os no-touch no-hd' }
  %head
    %title
      = @page_title
    = yield :included_stylesheets
    %style{ :type => 'text/css' }
      :plain
        html, body { margin: 0; padding: 0; border: 0; }
        body { font-family: Helvetica, "Tahoma Bold", Arial, sans-serif; font-size:16px; background: #fff; color: #505050; word-wrap: break-word; -webkit-text-size-adjust: none; -webkit-touch-callout: none; height: 100%; }
        h1, h2, h3, h4, h5, h6 { font-weight: normal; }
        p img { float: left; margin: 0 10px 5px 0; padding: 0; }
        img { border: 0; max-width: 100%; }
        table { width:auto; border-collapse: collapse;border-spacing: 0; }
        #top_box { height: 60px; margin: 0px; width: 100%; }
        #top_box,
        #bottom_box { background: transparent url(data:image/jpeg;base64,); }
        .explanation { color: 4f4f4f; clear: both; font-size: 14px; float: left; font-weight: bold; margin: 0px; margin-top: 6px; padding: 0px; text-align: center; width: 100%; }
        span.red { color: red; font-size: 16px; font-weight: normal; }
        #bottom_box { float: left; background-position: 0 -60px; cursor:pointer; height: 60px; margin: 0px; width: 100%; }
        #plus { background: url(data:image/png;base64,%3D); }
        #plus { float: left; margin: 13px 0 0 10px; width: 37px; height: 37px; }
        #load_more { color: #fff; font-size: 16px; font-weight: bold; float: left; margin-left: 6px; margin-top: 21px; text-decoration: none; text-shadow: 1px 1px 2px #3a3a3a; }

        .hidden { display: none; }
        .banner,
        .play { background: transparent url(data:image/png;base64,) no-repeat; }
        .banner { background-repeat: none; float: right; height: 39px; width: 39px; margin-top: -1px; overflow: hidden; }
        .cost { color: #fff; font-weight: bold; margin: 8px 0 0 11px; font-size: 13px; text-align: center; width: 30px; -o-transform: rotate(45deg); -moz-transform: rotate(45deg); -webkit-transform: rotate(45deg); -ms-transform: rotate(45deg); }
        .divider { background: #d8d8d8; float: left; width: 100%; height: 1px; width: 100%; }
        .offer { background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#f2f2f2), to(#fff)); box-shadow:inset 1px 1px 0px #fff; -webkit-box-shadow:inset 1px 1px 0px #fff; -o-box-shadow:inset 1px 1px 0px #fff; -moz-box-shadow:inset 1px 1px 0px #fff; -ms-box-shadow:inset 1px 1px 0px #fff; display: block; float: left; min-height: 76px; max-height: 120px; text-decoration: none; width: 100%; }
        .icon { float: left; margin: 9px; }
        .offer_icon { background-size: 57px 57px; height: 57px; width: 57px; }
        .video { height: 86px; }
        .video .icon { margin: 11px 9px 0 9px; }
        .video_icon { background-size: 100px 63px; width: 100px; height: 63px; }
        .play { background-position: -39px 0; float: left; width: 39px; height: 39px; margin: 10px 0 0 30px; }
        .offer-text { color: #7a7a7a; display: table-cell; font-size: 14px; height: 47px; margin: 0; padding: 10px 7px 0 0; vertical-align: middle; }
        .video .offer-text { padding-top: 13px; vertical-align: middle; }
        .offer-text span { color: #505050; font-size: 14px; font-weight: bold; }
        .offer-text span label { color: #0c429b; }
        .offer-text p { font-size: 12px; line-height: 14px; margin: 2px 0 0 0; }
        .ribbon { background: url(data:image/png;base64,) no-repeat; }
        .ribbon { color: #fff; float: right; font-size: 14px; font-weight: bold; height: 50px; line-height: 10px; margin: -60px 4px 0 0; width: 63px; }
        .ribbon span { float: right; text-align: center; width: 50px; height: 20px; left: 20px; top: 5px; line-height: 20px; }

        .load { float: right; height: 16px; width: 16px; margin: -30px 22px 0 0; }
        .offer.clicked .load { background: transparent url(data:image/gif;base64,) no-repeat 98%; }
        #footerWrapper { background: url(data:image/jpeg;base64,); }
        #footerWrapper { float: left; padding-bottom: 10px; width: 100%; }
        #footer { margin: 0px auto; margin-top: 10px; text-align: center; width: 100%; }
        #footer a, #footer span { display: inline-block; color: #fff; font-size: 13px; text-decoration: none; vertical-align: top; margin-top: 6px; height: 20px; }
        #footer #logo { background: transparent url(data:image/png;base64,) no-repeat; }
        #footer #logo { display: inline-block; width: 39px; height: 20px; margin-top: 6px; }

        .pagination a { color: #505050; font-size: 120%; font-weight: bold; display: block; }
        .pagination .prev { float: left; }
        .pagination .next { float: right; }
        .papaya-box { font-size: 12px; height: 25px; margin: 5px 5px 0 0; padding: 0; float: left;}
        .papaya-box span { font-size: 12px; color: #FFFFFF; font-weight: bold; text-decoration: none; text-align: center; white-space: nowrap; }
        .papaya-logo { height: 30px; width: 30px; background: url(data:image/png;base64,) no-repeat; background-size: 30px 30px; padding: 0 ; float: left; z-index: 1; position: relative; margin: -5px 0 0 -2px; }
        .orange-box { margin: 0 0 0 -15px; vertical-align: middle; display: inline-block; padding: 2px 7px 2px 14px; border: 1px solid; border-color: #C1791B; border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px; background: -moz-linear-gradient(top, #FFB323 0%, #F79216 100%) transparent; background: -webkit-gradient(linear, 0 0, 0 100%, color-stop(0, #FFB323), color-stop(1, #F79216)) transparent; -webkit-box-shadow: inset 0px 1px 0px #FFD688; -moz-box-shadow: inset 0px 1px 0px #FFD688; box-shadow: inset 0px 1px 0px #FFD688; text-shadow: 0px 1px 1px #9F5F0F; }
        .wifi_only { float: left; font-size: 10px; margin: 5px 5px 2px 0; padding: 0; font-weight: bold; color: #505050; }
        .wifi_icon { display: inline-block; height: 11px; width: 15px; margin: -2px 0; background:url(data:image/png;base64,) no-repeat scroll 0 0 transparent; }
      = yield :page_styles

    %meta{ 'http-equiv' => 'Content-Type', :content => 'text/html; charset=utf-8' }
    %meta{ 'name' => 'viewport', :content => 'width=device-width; initial-scale=1.0; maximum-scale=1.0; user-scalable=no;' }
    %style{ :type => 'text/css', :media => '(-webkit-min-device-pixel-ratio: 2)' }
      :plain
        .hd #top_box, .hd #bottom_box { background: transparent url(data:image/jpeg;base64,);}
        .hd #top_box { background-size: 60px auto; height: 60px; width: 100%; }
        .hd #bottom_box { background-position: 0 -60px; background-size: 60px auto; height: 60px; width: 100%; }
        .hd #plus { background: transparent url(data:image/png;base64,); }
        .hd #plus { background-size: 37px 37px; width: 37px; height: 37px; }
        .hd .banner, .hd .play { background: transparent url(data:image/png;base64,); }
        .hd .banner { background-size: 78px 40px; float: right; height: 39px; width: 39px; }
        .hd .play { background-repeat: none; background-size: 78px 40px; background-position: -39px 0; float: left; width: 39px; height: 39px; }
        .hd .ribbon { background: transparent url(data:image/png;base64,) no-repeat; }
        .hd .ribbon { background-size: 63px 22px; }
        .hd .offer.clicked .load { background: transparent url(data:image/gif;base64,) no-repeat 98%; }
        .hd #footerWrapper { background: url(data:image/jpeg;base64,); }
        .hd #footerWrapper { background-size: 45px auto; float: left; width: 100%; }
        .hd #footer #logo { background: transparent url(data:image/jpeg;base64,) no-repeat; }
        .hd #footer #logo { background-size: 36px 12px; background-position: bottom; }

  %body
    = yield

