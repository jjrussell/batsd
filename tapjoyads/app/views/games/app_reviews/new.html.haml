#review-form.current
  .panel
    .page-header
      .line-item.container.pb10
        .earn-main-app.left.ml5
          .squircle
            = image_tag(@app.get_icon_url(:size => 114, :source => :cloudfront))
        .ml5.mt5
          .big-title.slate
            = @app.name
          .sub-title
            = @app.partner_name
      .container
        .mod.menu-head
          = t("text.shared.rate_and_review")
        .mod.review-form
          = render 'form'
