xml.instruct!
xml.TapjoyConnectReturnObject do
  xml.PurchasedVGStoreItemsArray do
    unless @virtual_good_list.empty?
      xml << render(:partial => "virtual_good", :collection => @virtual_good_list, 
          :locals => {:point_purchases => @point_purchases})
    end
  end
  xml.MoreDataAvailable @more_data_available if @more_data_available
  xml.Success true
end