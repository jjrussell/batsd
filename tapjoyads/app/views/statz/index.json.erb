{
  "last_updated":"<%= @last_updated %>",
  "install_count_24hours":"<%= @install_count_24hours %>",
  "apps": [
    <% @app_list.length.times do |i| %>
      <% app = @app_list[i]; appstats = @appstats_list[i] %>
      {
        "name":"<%= app.name %>",
        "key":"<%= app.key %>",
        "icon_url":"<%= app.get_icon_url %>",
        "price":"<%= app.price %>",
        "payment_for_install":"<%= app.payment_for_install %>",
        "balance":"<%= app.balance %>",
        "daily_budget":"<%= app.daily_budget %>",
        "show_rate":"<%= app.show_rate %>",
        "stats": <%= appstats.stats.to_json %>
      }<%= ',' if i < @app_list.length - 1%>
    <% end %>
  ]
}